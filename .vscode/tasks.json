{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "build with gcc",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/main.c",
                "${workspaceFolder}/src/*.c",
                "-o",
                "${workspaceFolder}/build/knnsearch",
                "-I/opt/OpenBLAS/include",
                "-I${workspaceFolder}/include",
                "-I/usr/local/MATLAB/R2024b/extern/include",
                "-I/usr/lib/x86_64-linux-gnu/hdf5/serial/include",
                "-L/usr/local/MATLAB/R2024b/bin/glnxa64",
                "-L/opt/OpenBLAS/lib",
                "-L/usr/local/MATLAB/R2024b/sys/os/glnxa64",
                "-L/usr/lib/x86_64-linux-gnu/hdf5/serial",
                "-lstdc++",
                "-lopenblas",
                "-lm",
                "-lpthread",
                "-lmat",
                "-lmx",
                "-lhdf5"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}