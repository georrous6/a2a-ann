{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build Debug",
            "type": "shell",
            "command": "cmake -S . -B build -DCMAKE_BUILD_TYPE=Debug && cmake --build build",
            "problemMatcher": [],
            "group": "build"
        },
        {
            "label": "Build Release",
            "type": "shell",
            "command": "cmake -S . -B build -DCMAKE_BUILD_TYPE=Release -DPRECISION=DOUBLE && cmake --build build",
            "problemMatcher": [],
            "group": "build"
        }
    ]
}
