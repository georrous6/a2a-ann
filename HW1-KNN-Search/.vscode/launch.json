{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug knnsearch (gdb)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/knnsearch",
            "args": ["${workspaceFolder}/test/approx_tests/test01.mat", "C", "Q", "K", "IDX", "D", "--approx", "--verbose", "-o${fileDirname}/test/approx_tests/my_output.mat"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build with gcc"
        }
    ]
}